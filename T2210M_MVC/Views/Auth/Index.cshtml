@model List<T2210M_MVC.Models.UserRegisterModel>
@{
//var ls = ViewData["users"];
//var ls = ViewBag.users;
}
<div class="container">
    <h1>Danh sách người dùng (số lượng: @Model.Count)</h1>
    <table class="table table-striped table-bordered">
        <thead>
            <th>ID</th>
            <th>Email</th>
            <th>Full Name</th>
            <th>Telephone</th>
        </thead>
        <tbody>
            @foreach(T2210M_MVC.Models.UserRegisterModel item in Model)
            {
               <tr>
                   <td>@item.Id</td>
                   <td>@item.Email</td>
                   <td>@item.FullName</td>
                   <td>@item.Telephone</td>
               </tr> 
            }
        </tbody>
    </table>
</div>
